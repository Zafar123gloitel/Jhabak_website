{
  "name": "jhabak-share",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format-check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,mjs,cjs,css,scss,sass,md,html,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,mjs,cjs,css,scss,sass,md,html,json}\"",
    "lint-staged": "lint-staged --concurrent false",
    "prepare": "set NODE_ENV=development && rm -rf .husky && husky install && husky add .husky/pre-commit \"npm run lint-staged\" && husky add .husky/post-merge \"pnpm install\""
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint:fix",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "next": "14.1.0",
    "next-pwa": "^5.6.0",
    "node-sass": "^9.0.0",
    "react": "^18",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^18",
    "react-easy-crop": "^5.0.5",
    "react-icons": "^5.0.1",
    "react-jwt": "^1.2.0",
    "react-otp-input": "^3.1.1",
    "react-phone-input-2": "^2.15.1",
    "react-player": "^2.14.1",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "regexx": "^1.0.1",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.51",
    "@types/react-datepicker": "^4.19.5",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "react-datepicker": "^6.0.0",
    "typescript": "^5"
  }
}
